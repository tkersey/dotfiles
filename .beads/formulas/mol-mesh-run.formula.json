{
  "formula": "mol-mesh-run",
  "vars": {
    "target_id": {},
    "target_kind": {},
    "concurrency": {},
    "workspace_backend": {
      "default": "worktree"
    },
    "audit_level": {
      "default": "full"
    }
  },
  "steps": [
    {
      "id": "resolve-target",
      "title": "Resolve target",
      "gate": {
        "type": "manual"
      }
    },
    {
      "id": "validate-target",
      "title": "Validate target",
      "gate": {
        "type": "manual"
      },
      "depends_on": [
        "resolve-target"
      ]
    },
    {
      "id": "run-waves",
      "title": "Run waves",
      "depends_on": [
        "validate-target"
      ]
    },
    {
      "id": "closeout",
      "title": "Closeout",
      "depends_on": [
        "run-waves"
      ]
    }
  ]
}
